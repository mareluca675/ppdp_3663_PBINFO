#include <iostream>

using namespace std;

int main()
{
    int n, d, dpatrat;
    
    cin >> n;
    
    if(n % 2 == 0)
    {
        cout << 4;
    }
    else
    {
        for(d = 3; d * d <= n; d += 2)
    {
        {
            if(n % d == 0)
            {
                cout << d * d;
                break;
            }
        }
    }
    
    }
    return 0;
}
